<!DOCTYPE module SYSTEM "module.dtd">

<module name="sdlogger_spi" dir="loggers">
  <doc>
    <description>SPI SD Logger that saves pprzlog messages to SD Card.</description>
  </doc>
  <header>
    <file name="sdlogger_spi.h"/>
  </header>
  <init fun="sdlogger_spi_init()"/>
  <periodic fun="sdlogger_spi_periodic()" freq="512" start="sdlogger_spi_start()" stop="sdlogger_spi_stop()" autorun="FALSE"/>
  <makefile>
    <file name="sdlogger_spi.c"/>
  </makefile>
</module>

