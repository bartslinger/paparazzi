<!DOCTYPE settings SYSTEM "../settings.dtd">

<settings target="ap|nps">
  <dl_settings>

    <dl_settings NAME="INDI Heli LC">
      <dl_setting var="heli_indi_gains.roll_p" min="0" step="1" max="100"   module="stabilization/stabilization_attitude_heli_indi" shortname="pgain roll" param="STABILIZATION_ATTITUDE_HELI_INDI_ROLL_P" type="int32" persistent="true"/>
      <dl_setting var="heli_indi_gains.pitch_p" min="0" step="1" max="100"   module="stabilization/stabilization_attitude_heli_indi" shortname="pgain pitch" param="STABILIZATION_ATTITUDE_HELI_INDI_PITCH_P" type="int32" persistent="true"/>
      <dl_setting var="heli_indi_gains.yaw_p" min="0" step="1" max="100"   module="stabilization/stabilization_attitude_heli_indi" shortname="pgain yaw" param="STABILIZATION_ATTITUDE_HELI_INDI_YAW_P" type="int32" persistent="true"/>
      <dl_setting var="heli_indi_gains.yaw_d" min="0" step="1" max="100"   module="stabilization/stabilization_attitude_heli_indi" shortname="dgain yaw" param="STABILIZATION_ATTITUDE_HELI_INDI_YAW_D" type="int32" persistent="true"/>
    </dl_settings>

    <dl_settings NAME="INDI Actuators">
      <dl_setting max="1" min="0" step="1" var="heli_indi_ctl.enable_notch" shortname="notch" module="stabilization/stabilization_attitude_heli_indi" values="Off|On"/>
    </dl_settings>

    <dl_settings NAME="INDI Heli invG">
      <dl_setting var="heli_indi_ctl.roll_comp_angle" min="-50" step="0.5" max="50" module="stabilization/stabilization_attitude_heli_indi" shortname="roll.comp" type="int32" unit="raddinges" alt_unit="deg" alt_unit_coef="0.0139882"/>
      <dl_setting var="heli_indi_ctl.pitch_comp_angle" min="-50" step="0.5" max="50" module="stabilization/stabilization_attitude_heli_indi" shortname="pitch.comp" type="int32" unit="raddinges" alt_unit="deg" alt_unit_coef="0.0139882"/>
      <dl_setting var="heli_indi_ctl.invG[0][0]" min="0" step="1" max="100000"   module="stabilization/stabilization_attitude_heli_indi" shortname="[0][0]" type="int32"/>
      <dl_setting var="heli_indi_ctl.invG[0][1]" min="-100000" step="1" max="100000"   module="stabilization/stabilization_attitude_heli_indi" shortname="[0][1]" type="int32"/>
      <dl_setting var="heli_indi_ctl.invG[1][0]" min="-100000" step="1" max="100000"   module="stabilization/stabilization_attitude_heli_indi" shortname="[1][0]" type="int32"/>
      <dl_setting var="heli_indi_ctl.invG[1][1]" min="0" step="1" max="100000"   module="stabilization/stabilization_attitude_heli_indi" shortname="[1][1]" type="int32"/>
      <dl_setting var="heli_indi_ctl.invG[2][2]" min="0" step="1" max="10000"   module="stabilization/stabilization_attitude_heli_indi" shortname="[2][2]" type="int32"/>
      <!--dl_setting var="heli_indi_ctl.invG[2][3]" min="-500000" step="1" max="500000"   module="stabilization/stabilization_attitude_heli_indi" shortname="[2][3]" type="int32"/-->
      <!--dl_setting var="heli_indi_ctl.invG[3][3]" min="-500000" step="1" max="500000"   module="stabilization/stabilization_attitude_heli_indi" shortname="[3][3]" type="int32"/-->
    </dl_settings>

  </dl_settings>
</settings>
